"xbacklight -dec 3"
  XF86MonBrightnessDown
"xbacklight -inc 3"
  XF86MonBrightnessUp

# Increase volume
"pactl set-sink-volume 0 +3%"
   XF86AudioRaiseVolume

# Decrease volume
"pactl set-sink-volume 0 -3%"
   XF86AudioLowerVolume

# Mute/unmute volume
"pactl set-sink-mute 0 toggle"
   XF86AudioMute

